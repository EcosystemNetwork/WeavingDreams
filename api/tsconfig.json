{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "module": "CommonJS",
    "target": "ES2020",
    "lib": ["ES2020"],
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "noEmit": false,
    "outDir": ".vercel/output"
  },
  "include": [
    "**/*.ts",
    "../server/**/*.ts",
    "../shared/**/*.ts"
  ]
}
